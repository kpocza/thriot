<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="Replace">
    <!-- IIS - Dev Sql -->
    <add key="ManagementApi" value="http://localhost/api/v1" />
    <add key="PlatformApi" value="http://localhost/papi/v1" />
    <add key="ReportingApi" value="http://localhost/rapi/v1" />
    <add key="PlatformWebSocketApi" value="ws://localhost:8080" />
    <add key="sinkData" value="localSqlData"/>
    <add key="sinkTimeSeries" value="localSqlTimeSeries"/>
    <add key="paramSinkData" value="sqlData"/>
    <add key="paramSinkDataConnectionString" value="Server=.\SQLEXPRESS;Database=ThriotTelemetry;Trusted_Connection=True;"/>
  </appSettings>
</configuration>