<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="Replace">
    <!-- IIS - Prod Sql -->
    <add key="ManagementApi" value="http://thriotsqlweb.cloudapp.net/api/v1" />
    <add key="PlatformApi" value="http://thriotsqlweb.cloudapp.net/papi/v1" />
    <add key="ReportingApi" value="http://thriotsqlweb.cloudapp.net/rapi/v1" />
    <add key="PlatformWebSocketApi" value="ws://thriotsqlweb.cloudapp.net:8080" />
    <add key="sinkData" value="localSqlData"/>
    <add key="sinkTimeSeries" value="localSqlTimeSeries"/>
    <add key="paramSinkData" value="sqlData"/>
    <add key="paramSinkDataConnectionString" value="Server=.\SQLEXPRESS;Database=ThriotTelemetry;Trusted_Connection=True;"/>
  </appSettings>
</configuration>