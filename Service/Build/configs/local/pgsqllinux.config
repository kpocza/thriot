$microservice.messagingserviceendpoint=http://localhost/msvc/v1/messaging
$microservice.telemetrysetupserviceendpoint=http://localhost/papi/v1/telemetryDataSinkSetup

$publicurl.managementapi=http://localhost/api/v1
