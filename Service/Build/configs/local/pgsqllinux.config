$build.config=pgsql
$build.configmsg=pgsql
$build.copyConfigs=yes
$build.linuxify=yes
$build.queueconfig=no

$connectionstring.management=Server=127.0.0.1;Port=5432;Database=Thriot;User Id=thriot;Password=thriot;
$connectionstring.messaging=Server=127.0.0.1;Port=5432;Database=ThriotMessaging;User Id=thriotmessaging;Password=thriotmessaging;

$smtp.fromaddress=no-reply@thriot.io
$smtp.fromname=Thriot
$smtp.bouncesaddress=bounces@thriot.io
$smtp.host=smtp.sendgrid.net
$smtp.port=587
$smtp.username=***
$smtp.password=***

$queue.connectionstring=qcs
