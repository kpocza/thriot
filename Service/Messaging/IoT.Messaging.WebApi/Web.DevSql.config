<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="ManagementConnection" connectionString="Server=.\SQLEXPRESS;Database=Thriot;Trusted_Connection=True;" providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>

  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <container>
      <register type="IoT.Objects.Model.Operations.ISettingOperations,IoT.Objects.Model" mapTo="IoT.Objects.Operations.Sql.SettingOperations,IoT.Objects.Operations.Sql" xdt:Transform="Replace" xdt:Locator="Match(type)" />
      <register type="IoT.Framework.Azure.DataAccess.ICloudStorageClientFactory,IoT.Framework.Azure" xdt:Transform="Remove" xdt:Locator="Match(type)" />
      <register type="IoT.Objects.Operations.Sql.DataAccess.IObjectsUnitOfWorkFactory,IoT.Objects.Operations.Sql" mapTo="IoT.Objects.Operations.Sql.DataAccess.ObjectsUnitOfWorkFactory,IoT.Objects.Operations.Sql" xdt:Transform="Insert" />
    </container>
  </unity>
</configuration>