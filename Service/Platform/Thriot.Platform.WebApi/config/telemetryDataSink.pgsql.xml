    <sink name="pgSqlData" type="Thriot.Plugins.PgSql.TelemetryDataSinkCurrent, Thriot.Plugins.PgSql" description="PgSql table for data" />
    <sink name="pgSqlTimeSeries" type="Thriot.Plugins.PgSql.TelemetryDataSinkTimeSeries, Thriot.Plugins.PgSql" description="PgSql table for time series" />
    <sink name="localPgSqlData" type="Thriot.Plugins.PgSql.TelemetryDataSinkCurrent, Thriot.Plugins.PgSql" description="Locally-hosted PgSql table for data">
      <parameterPresets>
        <param key="ConnectionName" value="TelemetryConnection" />
        <param key="Table" value="CurrentData" />
      </parameterPresets>
    </sink>
    <sink name="localPgSqlTimeSeries" type="Thriot.Plugins.PgSql.TelemetryDataSinkTimeSeries, Thriot.Plugins.PgSql" description="Locally-hosted PgSql table for time series">
      <parameterPresets>
        <param key="ConnectionName" value="TelemetryConnection" />
        <param key="Table" value="TimeSeries" />
      </parameterPresets>
    </sink>
  </incoming>
</telemetryDataSink>
